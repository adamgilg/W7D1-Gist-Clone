<!DOCTYPE html>
<html>
<head>
  <title>GistClone</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% if logged_in? %>
    <div class="container">
      <%= link_to "Logout", session_path, method: :delete %>
    </div>
  <% end %>
  <div class="container">
    <%= yield %>
  </div>

</body>
</html>
